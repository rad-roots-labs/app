{
  "$schema": "../gen/schemas/capabilities.json",
  "identifier": "default",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-stat",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://*"
        },
        {
          "url": "http://127.0.0.1:*"
        }
      ],
      "deny": []
    },
    "notification:default",
    "os:default",
    "store:default",
    "shell:allow-open"
  ]
}