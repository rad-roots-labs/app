{
  "$schema": "./node_modules/turbo/schema.json",
  "ui": "tui",
  "globalEnv": [],
  "tasks": {
    "build": {
      "env": [
        "RADROOTS_APP_PORT"
      ],
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".svelte-kit/**",
        "dist",
        "dist-js"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}